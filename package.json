{
  "name": "docpile",
  "author": "jestann",
  "description": "Docpile UI",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/joliver/docpile-gui.git"
  },
  "engines": {
    "node": "10.4.1",
    "npm": "6.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.4.1",
    "react-pdf": "^4.0.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-spring": "^6.1.10",
    "react-table": "^6.8.6",
    "reactstrap": "^6.2.0",
    "sass": "^1.8.0",
    "semantic-ui-react": "^0.86.0"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/styles -o src/css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
